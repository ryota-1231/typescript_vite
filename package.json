{
  "name": "typescript_vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "tsc": "tsc",
    "tsc-watch": "tsc --watch",
    "serve": "vite preview",
    "eslint": "eslint --ignore-path .gitignore .  --ext .js,.jsx,.ts,.tsx",
    "fix:eslint": "eslint --fix --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint ./src/**/*.css, ./src/**/style.ts",
    "fix:stylelint": "stylelint --fix ./src/**/*.css, ./src/**/style.ts",
    "prettier": "prettier --check --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "fix:prettier": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.21",
    "@vitejs/plugin-react": "^1.1.4",
    "@storybook/react": "^6.4.18",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-tabs": "^3.2.3",
    "styled-components": "^5.3.3",
    "swiper": "^8.0.3",
    "vite": "^2.7.13"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@types/babel__core": "^7.1.18",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-tabs": "^2.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.5.1",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "^4.4.4",
    "webpack": "^5.68.0"
  }
